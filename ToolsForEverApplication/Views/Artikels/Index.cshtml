@model IEnumerable<ToolsForEverApplication.Models.Artikel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <button>
        @Html.ActionLink("Voeg een Artikel Toe", "Create")
    </button>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Product)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inkoopprijs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Verkoopprijs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fabriek.Fabriek1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Voorraad.Productcode)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Product)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inkoopprijs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Verkoopprijs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fabriek.Fabriek1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Voorraad.Productcode)
        </td>
        <td>
        </td>
    </tr>
}

</table>
